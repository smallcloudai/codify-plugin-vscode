				}
			},
			"codify-chat": {
				"description": "codify chat",
				"default": {
					"fontPath": "./assets/codify.woff",
					"fontCharacter": "\\e804"
				}
			},
			"codify-reload": {
				"description": "codify reload",
				"default": {
					"fontPath": "./assets/codify.woff",
					"fontCharacter": "\\e809"
				}
			},
			"codify-settings": {
				"description": "codify settings",
				"default": {
					"fontPath": "./assets/codify.woff",
					"fontCharacter": "\\e806"
				}
			},
			"codify-link": {
				"description": "codify link",
				"default": {
					"fontPath": "./assets/codify.woff",
					"fontCharacter": "\\e80A"
				}
			},
			"codify-send": {
				"description": "codify send",
